CREATE TABLE LAI_SUAT_TONG_HOP AS

select KY_HAN
,VND
,USD
,UPDATE_DATE
,PRIMARY_KEY
,BANK_NAME

FROM LAI_SUAT_AGRIBANK

UNION 
select KY_HAN
,VND
,USD
,UPDATE_DATE
,PRIMARY_KEY
,BANK_NAME
FROM LAI_SUAT_BIDV
UNION 
select KY_HAN
,VND
,USD
,UPDATE_DATE
,PRIMARY_KEY
,BANK_NAME
FROM LAI_SUAT_VIETCOMBANK
UNION
select KY_HAN
,VND
,USD
,UPDATE_DATE
,PRIMARY_KEY
,BANK_NAME
FROM LAI_SUAT_VIETINBANK
